<h1>Listing sensor_datas</h1>

<table>
  <tr>
    <th>Sensor</th>
    <th>Meas type</th>
    <th>Value</th>
    <th>Sequence</th>
    <th>Type</th>
    <th>Measured at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sensor_datas.each do |sensor_data| %>
  <tr>
    <td><%= sensor_data.sensor_id %></td>
    <td><%= sensor_data.meas_type_id %></td>
    <td><%= sensor_data.value %></td>
    <td><%= sensor_data.sequence %></td>
    <td><%= sensor_data.type %></td>
    <td><%= sensor_data.measured_at %></td>
    <td><%= link_to 'Show', sensor_data %></td>
    <td><%= link_to 'Edit', edit_sensor_data_path(sensor_data) %></td>
    <td><%= link_to 'Destroy', sensor_data, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Sensor data', new_sensor_data_path %>
